.Key file,opt1,opt2
assign lc: df1:c/
if not exists <file$t1>.c
   echo "File <file$t1>.c does not exist.  Try again."
   skip END
   endif

LC:lc1 <opt1> -cs -iDF1:include/lattice/ -iDF1:include/ -oRAM: <file$t1>
if not exists "RAM:<file$t1>.q"
   echo "Compile failed."
   quit 20
   endif

LC:lc2 <opt1> <opt2> -oDF0: RAM:<file$t1>
