{/////				Delay				/////}
{////////////////////////////////////////////////////////////////////}
{////////////////////////////////////////////////////////////////////}
{////////////////////////////////////////////////////////////////////}
{////////////////////////////////////////////////////////////////////}
{	          +---------------+                       }
{	          |        +------+                       }
{	        |VOL3|     |    |VOL2|                    }
{	          +--------+      V                       }
{	Input-----+---------------+-----|VOL1|-> Output   }

DelayInt:
	ar=DM(Input);
	sR=LSHIFT ar by 4 (HI);
	ay0=sr1;
	MY0=DM(VAR1);		{VOL1}
	MR=SR1 * MY0 (SU);
 
	ar=DM(i0,m0);
	MY0=DM(VAR2);		{VOL2}
	MR=MR+AR*MY0 (SU);
	if mv sat mr;

	si=mr1;

	ay1=DM(Output);
	ENA AR_SAT;
	ar=sr1+ay1;
	DIS AR_SAT;
	DM(Output)=ar;

	sR=ASHIFT ar by -1 (HI);

	MY0=DM(VAR3);		{VOL1}
	MR=AR * MY0 (SU);

	ENA AR_SAT;
	ar=mr1+ay0;
	DIS AR_SAT;

	DM(i0,m1)=ar;
	rts;
{////////////////////////////////////////////////////////////////////}
